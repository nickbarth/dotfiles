" .vimrc

set nowrap
set nu
set ruler
set ts=2
set bs=2
set shiftwidth=2
set wildmode=longest,list
set autoindent
set smarttab
set expandtab
set cindent
set showmatch
set novisualbell
set noerrorbells
set nohlsearch
set directory=/tmp/
set paste
set shortmess+=I
set list
set listchars=tab:..,trail:.
